{
  "root": "dist",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "redirects": {
    "/api/history/*": {
      "url": "https://emilios-ads-asst-v1-history-backend.onrender.com/api/history/:splat",
      "status": 200
    },
    "/api/*": {
      "url": "https://emilios-ads-asst-v1-backend.onrender.com/api/:splat",
      "status": 200
    },
    "/analyze/*": {
      "url": "https://emilios-ads-asst-v1-backend.onrender.com/analyze/:splat",
      "status": 200
    },
    "/get-help/*": {
      "url": "https://emilios-ads-asst-v1-backend.onrender.com/get-help/:splat",
      "status": 200
    }
  }
} 